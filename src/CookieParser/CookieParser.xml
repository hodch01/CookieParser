<?xml version="1.0" encoding="utf-8" ?>
<widget id="CookieParser.widget.CookieParser" needsEntityContext="true" xmlns="http://www.mendix.com/widget/1.0/">
  <name>CookieParser</name>
  <description>Parsers a cookie and sets its value in an attribute</description>

  <icon></icon>

  <properties>
        <property key="cookieAttribute" type="attribute">
            <caption>Attribute</caption>
            <category>Configuration</category>
            <description>Attribute to be set by cookie</description>
            <attributeTypes>
                <attributeType name="String"/>
            </attributeTypes>
        </property>
        <property key="cookieName" type="string" required="true" defaultValue="">
            <caption>Cookie name</caption>
            <category>Configuration</category>
            <description>Name of the cookie</description>
        </property>
	</properties>
</widget>

